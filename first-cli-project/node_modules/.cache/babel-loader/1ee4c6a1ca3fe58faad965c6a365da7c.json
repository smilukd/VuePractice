FD,KAXV;AAeH,IAAA,YAAY,EAAE;AACV,MAAA,IAAI,EAAE,MADI;AAEV,MAAA,QAAQ,EAAE;AAFA,KAfX;AAmBH,IAAA,UAAU,EAAE;AACR,MAAA,IAAI,EAAE,OADE;AAER,MAAA,QAAQ,EAAE,KAFF;AAGR,iBAAS;AAHD;AAnBT,GARI;AAkCX;AAEA;AACA;AACA;AACA;AAEA;AACR;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACQ,EAAA,IApDW,kBAoDJ;AACH,WAAO;AACH,MAAA,iBAAiB,EAAE;AADhB,KAAP;AAGH,GAxDU;AAyDX,EAAA,OAAO,EAAE;AACL,IAAA,aADK,2BACW;AACZ,WAAK,iBAAL,GAAyB,CAAC,KAAK,iBAA/B;AACH,KAHI;AAIL,IAAA,cAJK,4BAIY;AACb;AACA,WAAK,KAAL,CAAW,iBAAX,EAA8B,KAAK,EAAnC;AACJ;AAPK;AAzDE,CAAf","sourcesContent":["<template>\n    <li>\n        <h2>{{ name }} {{ isFavorite ? '( FAVORITE )' : '' }}</h2>\n        <button @click=\"toggleFavorite\">Toggle Favorite</button>\n        <button @click=\"toggleDetails\">{{ detailsAreVisible ? 'Hide' : 'Show' }} Details</button>\n        <ul v-if=\"detailsAreVisible\">\n            <li><strong>Phone: </strong>{{ phoneNumber }}</li>\n            <li><strong>Email: </strong>{{ emailAddress }}</li>\n        </ul>\n    </li>\n</template>\n\n\n<script>\n    export default {\n        // one way to define an array of props\n        //        props: [\n        //            'name',\n        //            'phoneNumber',\n        //            'emailAddress',\n        //            'isFavorite'\n        //        ],\n        props: {\n            // another way to define an array of props, as objects, with details about its data type, required, and even validation\n            id: {\n                type: String,\n                required: true\n            },\n            name: {\n                type: String,\n                required: true,\n\n            },\n            phoneNumber: {\n                type: String,\n                required: true\n            },\n            emailAddress: {\n                type: String,\n                required: true\n            },\n            isFavorite: {\n                type: Boolean,\n                required: false,\n                default: false,\n            }\n        },\n\n        //user emits to help define and make clear what is being emitted by this vue\n\n        //this is the most basic, it only lists that there is a toggle-favorite being emitted that we can listen for\n        //        emits: [\n        //            'toggle-favorite'\n        //        ],\n\n        // this version is a bit more verbose, it tells us the name to listen for, and also what is being done with that emitter\n//        emits: {\n//            'toggle-favorite': functon(id) {\n//                if (id) {\n//                    return true;\n//                } else {\n//                    return false;\n//                }\n//\n//            }\n//        }\n        data() {\n            return {\n                detailsAreVisible: false\n            };\n        },\n        methods: {\n            toggleDetails() {\n                this.detailsAreVisible = !this.detailsAreVisible;\n            },\n            toggleFavorite() {\n                // emit custom event when the favorit toggle button is clicked.\n                this.$emit('toggle-favorite', this.id);\n            }\n        }\n    }\n\n</script>\n"],"sourceRoot":""}]}